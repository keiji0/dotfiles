! rxvt
urxvt.saveLines: 0
urxvt*termName:screen-256color
urxvt.scrollBar: off
urxvt.cursorBlink: false
urxvt.preeditType: OverTheSpot
urxvt.letterSpace: -1
urxvt.shading: 10
urxvt.fading: 40
!urxvt.lineSpace: -3
urxvt.lineSpace: 1
!urxvt.font: xft:Inconsolata:size=11,xft:TakaoGothic:size=10
!urxvt.boldFont: xft:Inconsolata:size=11,xft:TakaoGothic:size=10
urxvt.font: xft:Ricty for Powerline:size=12:hinting=true
urxvt.boldFont: xft:Ricty for Powerline:size=12:hinting=true
urxvt.background: #212224
urxvt.foreground: light grey
urxvt.underlineColor: #999999

!font
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

*background:        #211708
*foreground:        #978f83

